<UserControl x:Class="ShortestCycleDirGraph.Pages.HomePages.ProjectAbout"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <ScrollViewer>
            <StackPanel MinWidth="200">
                <TextBlock Text="Temat" Style="{StaticResource Heading1}" />
                <TextBlock x:Name="TopicContent" Text="" FontSize="14"/>
                <TextBlock Text=" Zespół" Style="{StaticResource Heading2}" Margin="0,40,0,0" />
                <TextBlock x:Name="TeamContent" FontSize="14"/>

            </StackPanel>
        </ScrollViewer>
    </Grid>
</UserControl>
