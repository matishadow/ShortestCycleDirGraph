<UserControl x:Class="ShortestCycleDirGraph.Pages.Disp.Display"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <ScrollViewer>
            <StackPanel MinWidth="200">
                <TextBlock x:Name="DisplayTitle" Text="Wprowadzony graf" Style="{StaticResource Heading2}" />
                <Image x:Name="GraphImage" Width="450" Height="450" HorizontalAlignment="Left" Margin="0,20,0,0"/>

            </StackPanel>
        </ScrollViewer>
    </Grid>
</UserControl>
